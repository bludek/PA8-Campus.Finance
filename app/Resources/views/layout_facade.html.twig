<!doctype html>
<head>
	<meta charset="UTF-8">
	<title>{% block title %}Campus finance{% endblock %}</title>
	
	{% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('bundles/sdzblog/css/styles_facade.css') }}" type="text/css" media="screen" />
            <link rel="stylesheet" href="{{ asset('bundles/sdzblog/css/jquery-ui-1.8.18.custom.css') }}" type="text/css" media="screen" />
	{% endblock %}
	
	<link rel="icon" href="images/favicon.gif" type="image/x-icon"/>
 	<!--[if lt IE 9]>
			<script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
 	<![endif]-->
 	<link rel="shortcut icon" href="images/favicon.gif" type="image/x-icon"/>
	
	{% block javascripts %}
            <script src="{{ asset('bundles/sdzblog/js/jquery-1.7.1.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/sdzblog/js/jquery-ui-1.8.18.custom.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('bundles/sdzblog/js/i18n/jquery-ui-i18n.js') }}" type="text/javascript"></script>
        {% endblock %}
</head>
	
<body>
	<!--start container-->
    <div id="container">

    <!--start header-->
    <header>
 
	
   	<!--start menu-->
        <nav style="border:3px double black;">
    	<ul>
    		<li><a href="{{ path('Home') }}">Home</a></li>
    		{% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
    			<li><a href="{{ path('SdzSitesBundle_homepage') }}">Sites</a></li>
    		{% else %}
    			<li><a href="{{ path('fos_user_security_login') }}">Sites</a></li>
    		{% endif %}	
    		<li><a href="{{ path('Home') }}">About</a></li>
    		<li><a href="{{ path('article') }}">News</a></li>
    		<li><a href="{{ path('Home') }}">Contact</a></li>
    	</ul>
    </nav>
	<!--end menu-->
	   
	</header>
	<!--end header-->
	<div>
		{% block body %}
		{% endblock %}
	</div>
	
	</div>
   <!--end container-->
 
	<!--start footer-->
   	<footer>
   	<div class="container">
   	<section class="footer_left">
   		<h3>Contact information:
   			<span>Tel : 123 333 4444</span>
   			<span>Mail : email@website.com</span>
   		</h3>
  	</section> 
   
   	<section class="footer_left">
   		<h3>Website:
   			<span>www.goldenboys.fr</span>
   			<span>www.boursematch.com</span>
   		</h3>
   	</section> 

   	<aside class="footer_left">
   		<h3>Compatibility:
   			<span><img src="{{ asset('bundles/sdzblog/images/browsers.png') }}" alt="compatibility" class="icons"/></span>
   			<span>Valid html5 / design and code</span>
   		</h3>
   	</aside> 
   	<img src="{{ asset('bundles/sdzblog/images/contact-us.png') }}" width="240" height="230" alt="contact" class="picture_footer"/>

   	<div id="FooterTwo"> Copyright &copy; 2011 - All Rights Reserved </div>
   	<div id="FooterTree"> Design by <a href="#">Domain Name</a> </div> 
   	</div>
   	</footer>
  	<!--end footer-->
 	<!-- Free template distributed by http://freehtml5templates.com -->  
</body>
</html>